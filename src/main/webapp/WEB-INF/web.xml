		<servlet-class>com.algonquin.capstone.servlets.CreateQuestionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateQuestionServlet</servlet-name>
		<url-pattern>/CreateQuestionServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description></description>
		<display-name>QuestionManageServlet</display-name>
		<servlet-name>QuestionManageServlet</servlet-name>
		<servlet-class>com.algonquin.capstone.servlets.QuestionManageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QuestionManageServlet</servlet-name>
		<url-pattern>/QuestionManageServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description></description>
		<display-name>DeleteQuestionServlet</display-name>
		<servlet-name>DeleteQuestionServlet</servlet-name>
		<servlet-class>com.algonquin.capstone.servlets.DeleteQuestionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteQuestionServlet</servlet-name>
		<url-pattern>/DeleteQuestionServlet</url-pattern>
	</servlet-mapping>
	
	
