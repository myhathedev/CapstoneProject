		<servlet-class>com.algonquin.capstone.servlets.UpdateQuestionServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>UpdateQuestionServlet</servlet-name>
		<url-pattern>/UpdateQuestionServlet</url-pattern>
	</servlet-mapping>
	
	
	 <servlet>
       <servlet-name>ExamListServlet</servlet-name>
       <servlet-class>com.example.ExamListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
       <servlet-name>ExamListServlet</servlet-name>
       <url-pattern>/ExamList</url-pattern>
    </servlet-mapping>

</web-app>